{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_display_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_authors}"
    ],
    "contact": {
        "homepage": "https://asdf.zip/",
        "sources": "https://asdf.zip/FabricMC/fabric-example-mod"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "de.abq.arcane_divinity.ArcaneDivinityEntry"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version_range}",
        "fabric-api": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=21"
    }
}
  